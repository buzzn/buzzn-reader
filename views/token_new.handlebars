<form name="new token", method="post", action="/token/create">

  <div class="form-group row">
    <label for="inputToken" class="col-sm-2 form-control-label">Token</label>
    <div class="col-sm-10">
      <input name="token", type="text" class="form-control">
    </div>
  </div>


  <div class="form-group row">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-secondary">Connect</button>
    </div>
  </div>

</form>
